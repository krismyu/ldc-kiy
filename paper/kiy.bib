	Date-Modified = {2013-06-14 14:12:06 -0700},
	Keywords = {Cantonese, tone, intonation},
	Publisher = {Lincom Europa},
	Title = {Intonation in {C}antonese},
	Year = {2003},
	Bdsk-File-1 = {}}


	Journal = {Language and Speech},
	Keywords = {tone, Thai},
	Month = sep,
	Number = {3},
	Pages = {343--383},
	Title = {Tone Features, Tone Perception, and Peak Alignment in Thai},
	Url = {http://las.sagepub.com/cgi/content/abstract/50/3/343},
	Volume = {50},

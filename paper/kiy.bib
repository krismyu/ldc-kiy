@article{Teller:1984,
	Abstract = {Linking propositions are statements that relate perceptual states to physiological states, and as such are one of the fundamental building blocks of visual science. A brief history of the concept of linking proposition is presented. Five general families of linking {propositions---Identity}, Similarity, Mutual Exclusivity, Simplicity and Analogy---are discussed, and examples of each are developed. Two specific linking propositions, involving the explanation of perceptual phenomena on the basis of the activity of single neurons, are explicated and their limitations are explored in detail. Finally, the question of the empirical testability and falsifiability of linking propositions is discussed.},
	Author = {Teller, Davida Y.},
